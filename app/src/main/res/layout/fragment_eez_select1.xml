<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="right"
            type="com.gugu.wegolsu.k_seastem.fragment.EEZRightMenuFragment" />
    </data>

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/constSelect1">

        <ImageButton
            android:background="@drawable/eez_1"
            android:id="@+id/imbEEZMenuLeft1"
            android:layout_height="0dp"
            android:layout_width="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/imbEEZMenuRight1"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:background="@drawable/eez_2"
            android:id="@+id/imbEEZMenuRight1"
            android:layout_height="0dp"
            android:layout_width="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/imbEEZMenuLeft1"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </android.support.constraint.ConstraintLayout>

</layout>